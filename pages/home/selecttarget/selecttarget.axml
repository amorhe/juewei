      <view class="addressTxt">
        <view class="fontBold">紫檀大厦 606</view>
        <view class="addressTabbar bar1">学校</view>
      </view>
      <view class="fontGray">小春（先生） 188 8888 8888</view>
    </view>

    <view>
      <view class="addressTxt">
        <view class="fontBold">紫檀大厦 606</view>
        <view class="addressTabbar bar2">公司</view>
      </view>
      <view class="fontGray">小春（先生） 188 8888 8888</view>
    </view>

    <view>
      <view class="addressTxt">
        <view class="fontBold">紫檀大厦 606</view>
        <view class="addressTabbar bar3">家</view>
      </view>
