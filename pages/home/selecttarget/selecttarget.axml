<view class="searchView">
  <view>
    <view class="positionAddress">
      <text>北京市</text>
      <image src="{{imageUrl}}arrow_down1.png" />
    </view>
    <view class="searchBox">
      <image class="searchIcon" src="{{imageUrl}}search.png" />
      <input type="text" placeholder="小区/写字楼/学校等" placeholder-class="colorGray"  />
    </view>
  </view>
</view> 

<view class="positionIng">
  <view class="fontGray">当前定位</view>
  <view class="positionTxt">
    <view>
      <image src="{{imageUrl}}position.png"/>
      <text>紫檀大厦</text>
    </view>
    <view class="numberColor fontTwentySix">重新定位</view>
  </view>
</view>

<!-- 收货地址 -->
<view class="delivery">
  <view class="fontGray">收货地址</view>
  <view class="addressList">
